{
  "name": "initxlab/ravendb",
  "description": "RavenDB PHP API Client",
  "type": "project",
  "require": {
    "php": "^8.0",
    "ext-curl": "*",
    "ext-json": "*",
    "symfony/http-client": "^5.2.3",
    "analog/analog": "dev-master",
    "ramsey/uuid": "4.x-dev",
    "symfony/serializer": "5.x-dev",
    "symfony/property-access": "5.x-dev"
  },
  "autoload": {
    "psr-4": {
      "RavenDB\\": "src/ravendb/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "RavenDB\\Tests\\": "src/tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "symfony/var-dumper": "5.x-dev"
  },
  "license": "MIT",
  "minimum-stability": "dev"
}
